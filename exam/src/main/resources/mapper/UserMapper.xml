<mapper namespace="com.xio.mapper.UserMapper">

    <select id="checkUser" parameterType="com.xio.pojo.SUser" resultType="com.xio.pojo.SUser">
        SELECT * FROM  s_user WHERE username=#{username} AND password=#{password}
    </select>

</mapper>